@model IEnumerable<BookModel>
<div class="container">
    <h3 class="display-4">All Book List</h3>
    <div class="row">
        @foreach (var book in Model)
        {
          <div class="col-md-4">
              <partial name="_book" model="@book"/>
          </div>  

        }

    </div>
</div>